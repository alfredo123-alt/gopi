# Define the two vectors
A <- c(1, 2, 3, 4, 5, 6)
B <- c(7, 8, 9, 10, 11, 12)

# Combine vectors into an array of dimensions 3×3×2
result_array <- array(c(A, B), dim = c(3, 3, 2))

# Print the final array
print("3x3x2 Array:")
print(result_array)

START

    DEFINE vector A = (1,2,3,4,5,6)
    DEFINE vector B = (7,8,9,10,11,12)

    COMBINE A and B into a 3x3x2 array
    PRINT the final array

END
